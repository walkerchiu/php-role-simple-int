{
    "name": "walkerchiu/php-role-simple-int",
    "type": "library",
    "authors": [
        {
            "name":  "Walker Chiu",
            "email": "chenjen.chiou@gmail.com"
        }
    ],
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.3",
        "walkerchiu/php-core-int": "^1.0"
    },
    "suggest": {
        "walkerchiu/php-account-int": "^1.0",
        "walkerchiu/php-role-int": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "WalkerChiu\\RoleSimple\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "WalkerChiu\\RoleSimple\\RoleSimpleServiceProvider"
            ],
            "aliases": {
                "Role":       "WalkerChiu\\RoleSimple\\Models\\Entities\\Role",
                "Permission": "WalkerChiu\\RoleSimple\\Models\\Entities\\Permission"
            }
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    }
}
